{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["airbnb-base", "prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "import/first": 2,
    "indent": ["error", 4],
    "quotes": [2, "double"],
    "semi": [2, "never"],
    "import/extensions": [
      0,
      "ignorePackages",
      {
        "js": "never"
      }
    ],
    "no-param-reassign": 2,
    "no-unused-vars": [2, { "argsIgnorePattern": "next" }]
  }
}